@model HelloDoc.DAL.ViewModels.ProviderProfilevm;


<div class="container-fluid">
    <div class="row d-flex justify-content-center">
        <div class="col-md-8 col-11">
            <div class="d-flex  mb-3 justify-content-between">
                <div class="mt-3">
                    <h4>
                        Edit Physician Account
                    </h4>
                </div>
                @if (Model.callId == 1)
                {
                    <a class="btn btn-outline-info border border-2 border-info fs-5 p-2 rounded fw-semibold me-2" onclick="ProviderList(0)">
                        <i class="bi bi-caret-left"></i> Back
                    </a>
                }
                @if (Model.callId == 2)
                {
                    <a class="btn btn-outline-info border border-2 border-info fs-5 p-2 rounded fw-semibold me-2" onclick="GetUserAccess(0)">
                        <i class="bi bi-caret-left"></i> Back
                    </a>
                }
            </div>
            <div class="shadow p-4">
                @if (Model.callId == 3)
                {
                    <div class="d-flex justify-content-end">
                        <button type="button" id="requestToAdmin" class="btn btn-outline-info border border-2 border-info fs-5 p-2 rounded fw-semibold me-2" data-bs-toggle="modal" data-bs-target="#requestToAdminModal">
                            Request To Admin
                        </button>
                    </div>
                }
                <form method="post" id="PhysicianAccountForm" class="allForm" onsubmit="PhysicianAccountEdit(@Model.callId)">
                    <input asp-for="@Model.AspId" class="editfield1" id="aspId1" hidden/>
                    <input asp-for="@Model.PhysicianId" class="editfield1" id="physicianId1" hidden/>                

                    <div class="row">
                        <div class="col-xl-12 ">
                            <h5>Account Information</h5>
                        </div>
                        <div class="col-xl-6 mt-2">
                            <div class="form-floating">
                                <input type="text" asp-for="@Model.Username" class="form-control " id="floatingUserName" placeholder="User Name">
                                <label for="floatingUserName" class="text-secondary">User Name</label>
                            </div>
                        </div>
                        <div class="col-xl-6 mt-xl-2 mt-4">
                            <div class="form-floating">
                                <input type="password" asp-for="@Model.Password" class="form-control" id="floatingPassword2" placeholder="Password" autocomplete="on">
                                <label for="floatingPassword" class="text-secondary">Password</label>
                                <span class="text-danger" asp-validation-for="@Model.Password"></span>
                                <div class="eye-icon position-absolute top-0 end-0 mx-3 my-2">
                                    <i class="bi bi-eye-slash fs-4 text-muted mt-1 common-cursor-pointer" id="togglePassword2" onclick="showPassword()"></i>
                                </div>
                            </div>
                        </div>
                        <div class="col-xl-6 mt-xl-2 mt-4 ">
                            <div class="form-floating">
                                <select class="form-select editfield1" id="status" asp-for="@Model.Status" aria-label="Status">
                                    <option  disabled>Select Your Status</option>
                                    <option value="1">Active</option>
                                    <option value="2">Pending</option>
                                    <option value="3">Not Active</option>
                                </select>
                                <label for="status">Status</label>
                            </div>
                        </div>
                        <div class="col-xl-6 mt-xl-2 mt-4 ">
                            <div class="form-floating">
                                <select class="form-select editfield1" id="Role" aria-label="Role" asp-for="@Model.RoleId">
                                    @foreach (var obj in Model.Roles)
                                    {
                                        <option value="@obj.RoleId">
                                            @obj.Name
                                        </option>
                                    }
                                </select>
                                <label for="Role">Role</label>
                            </div>
                        </div>
                        <div class="col-12 mt-4 d-flex justify-content-end">
                            @if (Model.callId != 3)
                            {
                                <button type="button" id="editbtn1" class="border-0 bg-transparent">
                                    <a class="btn  btn-info text-white fs-5  py-2 px-4 rounded fw-semibold me-2">
                                        Edit
                                    </a>
                                </button> 
                            }
                            <button type="button" id="resetpassbtn" class="border-0 bg-transparent" onclick="PhysicianProfileResetPassword(@Model.callId)">
                                <a class="btn btn-outline-info border border-2 border-info fs-5 p-2 rounded fw-semibold me-2">
                                    Reset Password
                                </a>
                            </button>
                            <button type="submit" id="savebtn1" class="border-0 bg-transparent" style="display:none">
                                <a class="btn  btn-info text-white fs-5  py-2 px-4 rounded fw-semibold me-2">
                                    Save
                                </a>
                            </button>
                            <button type="reset" id="cancelbtn1" class="border-0 bg-transparent" style="display:none">
                                <a class="btn btn-outline-info border border-2 border-info fs-5  p-2 rounded fw-semibold">
                                    Cancel
                                </a>
                            </button>
                        </div>

                    </div>
                </form>
                <form class="allForm" id="PhysicianAdministratorForm" method="post" onsubmit="PhysicianAdministratorEdit(@Model.callId)">
                    <input asp-for="@Model.AspId" class="editfield2" id="aspId2" hidden />
                    <input asp-for="@Model.PhysicianId" class="editfield2" id="physicianId2" hidden />
                    <div class="row">
                        <div class="col-12 mt-4">
                            <h5>Physician Information</h5>
                        </div>
                        <div class="col-xl-6 mt-2">
                            <div class="form-floating">
                                <input type="text" asp-for="@Model.FirstName" class="form-control editfield2" id="floatingFirstName15" placeholder="First Name">
                                <label for="floatingFirstName15" class="text-secondary">First Name</label>
                                <span class="text-danger" asp-validation-for="FirstName"></span>
                            </div>
                        </div>
                        <div class="col-xl-6 mt-xl-2 mt-4 ">
                            <div class="form-floating">
                                <input type="text" asp-for="@Model.LastName" class="form-control editfield2" id="floatingLastName15" placeholder="Last Name">
                                <label for="floatingLastName15" class="text-secondary">Last Name</label>
                                <span class="text-danger" asp-validation-for="LastName"></span>

                            </div>
                        </div>
                        <div class="col-xl-6 mt-4">
                            <div class="form-floating">
                                <input type="email" asp-for="@Model.Email" class="form-control editfield2" id="floatingEmail15" placeholder="Email">
                                <label for="floatingEmail15" class="text-secondary">Email</label>
                                <span class="text-danger" asp-validation-for="Email"></span>

                            </div>
                        </div>
                        <div class="col-xl-6 mt-4">
                            <div class="form-floating">
                                <input type="tel" asp-for="@Model.Phonenumber" class="form-control editfield2" id="phone25" placeholder="PhoneNumber" />
                                <label for="phone25" class="text-secondary">PhoneNumber</label>
                                <span class="text-danger" asp-validation-for="Phonenumber"></span>
                            </div>
                        </div>
                        <div class="col-xl-6 mt-4">
                            <div class="form-floating">
                                <input type="text" asp-for="@Model.MedicalLicense" class="form-control editfield2" id="floatingLicence" placeholder="Medical License #">
                                <label for="floatingLicence" class="text-secondary">Medical License #</label>
                                <span class="text-danger" asp-validation-for="MedicalLicense"></span>

                            </div>
                        </div>
                        <div class="col-xl-6 mt-4">
                            <div class="form-floating">
                                <input type="text" asp-for="@Model.NPINumber" class="form-control editfield2" id="floatingNPI" placeholder="NPI Number">
                                <label for="floatingNPI" class="text-secondary">NPI Number</label>
                                <span class="text-danger" asp-validation-for="NPINumber"></span>
                            </div>
                        </div>
                        <div class="col-xl-6 mt-4">
                            <div class="form-floating">
                                <input type="email" asp-for="@Model.SyncEmail" class="form-control editfield2" id="SyncEmail" placeholder="SyncEmail">
                                <label for="SyncEmail" class="text-secondary">Synchronization Email Address</label>
                                <span class="text-danger" asp-validation-for="SyncEmail"></span>
                            </div>
                        </div>
                        <div class="col-xl-6 mt-4">
                            @foreach (var obj in Model.PhysicianRegionTables)
                            {
                                <div class="form-check form-check-inline pb-1 mx-3">
                                    <input class="form-check-input editfield2" name="physicianRegions" type="checkbox" @(obj.ExistsInTable ? "checked" : "") id="@obj.Regionid" value="@obj.Regionid">
                                    <label class="form-check-label" for="@obj.Regionid">@obj.Name</label>
                                </div>
                            }                    
                        </div>
                        <div class="col-12 d-flex justify-content-end  mt-4">                          
                            @if (Model.callId != 3)
                            {
                                <button type="button" id="Payratebtn" class="border-0 bg-transparent" onclick="Payrate('@Model.AspId',@Model.PhysicianId,@Model.callId)">
                                    <a class="btn  btn-info text-white fs-5  py-2 px-4 rounded fw-semibold me-2">
                                        Enter Payrate
                                    </a>
                                </button>

                                <button type="button" id="editbtn2" class="border-0 bg-transparent">
                                    <a class="btn  btn-info text-white fs-5  py-2 px-4 rounded fw-semibold me-2">
                                        Edit
                                    </a>
                                </button>
                            }
                            <button type="submit" id="savebtn2" class="border-0 bg-transparent" style="display:none">
                                <a class="btn  btn-info text-white fs-5  py-2 px-4 rounded fw-semibold me-2">
                                    Save
                                </a>
                            </button>
                            <button type="reset" id="cancelbtn2" class="border-0 bg-transparent" style="display:none">
                                <a class="btn btn-outline-info border border-2 border-info fs-5  p-2 rounded fw-semibold">
                                    Cancel
                                </a>
                            </button>
                        </div>
                    </div>
                </form>

                <form class="allForm" id="PhysicianMailingForm" method="post" onsubmit="PhysicianMailingEdit(@Model.callId)">
                    <input asp-for="@Model.AspId" class="editfield3" id="aspId3" hidden />
                    <input asp-for="@Model.PhysicianId" class="editfield3" id="physicianId3" hidden />
                    <div class="row">
                        <div class="col-12 mt-4">
                            <h5>Mailing And Billing Information</h5>
                        </div>
                        <div class="col-xl-6 mt-2">
                            <div class="form-floating">
                                <input type="text" asp-for="@Model.Address1" class="form-control editfield3" id="floatingAddress1" placeholder="Address1">
                                <label for="floatingAddress1" class="text-secondary">Address1</label>
                                <span class="text-danger" asp-validation-for="Address1"></span>
                            </div>
                        </div>
                        <div class="col-xl-6 mt-4 mt-xl-2">
                            <div class="form-floating">
                                <input type="text" asp-for="@Model.Address2" class="form-control editfield3" id="floatingAddress2" placeholder="Address2">
                                <label for="floatingAddress2" class="text-secondary">Address2</label>
                                
                            </div>
                        </div>
                        <div class="col-xl-6 mt-4 ">
                            <div class="form-floating">
                                <input type="text" asp-for="@Model.City" class="form-control editfield3" id="floatingCity" placeholder="City">
                                <label for="floatingCity" class="text-secondary">City</label>
                                <span class="text-danger" asp-validation-for="City"></span>
                            </div>
                        </div>
                        <div class="col-xl-6 mt-4 ">
                            <div class="form-floating">
                                <select class="form-select editfield3" id="state" aria-label="Region" asp-for="@Model.RegionId">
                                    @foreach (var obj in Model.Regions)
                                    {
                                        <option value="@obj.RegionId">
                                            @obj.Name
                                        </option>
                                    }
                                </select>                                          
                                <label for="state">State</label>
                                <span class="text-danger" asp-validation-for="RegionId"></span>
                            </div>
                        </div>

                        <input type="text" class="editfield3" id="longitude" asp-for="@Model.Longitude" hidden />
                        <input type="text" class="editfield3" id="latitude" asp-for="@Model.Latitude" hidden />
                        <div class="col-xl-6 mt-4 ">
                            <div class="form-floating">
                                <input type="text" asp-for="@Model.Zipcode" class="form-control editfield3" id="physicianZipCode" placeholder="Zip" onblur="getcoordinates()">
                                <label for="physicianZipCode" class="text-secondary">Zip</label>
                                <span class="text-danger" asp-validation-for="Zipcode"></span>
                            </div>
                        </div>

                        <div class="col-xl-6 mt-4">
                            <div class="form-floating">
                                <input type="tel" asp-for="@Model.AltPhone" class="form-control editfield3" id="phone26" placeholder="PhoneNumber" />
                                <label for="phone26" class="text-secondary">PhoneNumber</label>
                                <span class="text-danger" asp-validation-for="AltPhone"></span>
                            </div>
                        </div>
                        <div class="col-12 d-flex justify-content-end  mt-4">
                            @if (Model.callId != 3)
                            {
                                <button type="button" id="editbtn3" class="border-0 bg-transparent">
                                    <a class="btn  btn-info text-white fs-5  py-2 px-4 rounded fw-semibold me-2">
                                        Edit
                                    </a>
                                </button>
                            }
                            <button type="submit" id="savebtn3" class="border-0 bg-transparent" style="display:none">
                                <a class="btn  btn-info text-white fs-5  py-2 px-4 rounded fw-semibold me-2">
                                    Save
                                </a>
                            </button>
                            <button type="reset" id="cancelbtn3" class="border-0 bg-transparent" style="display:none">
                                <a class="btn btn-outline-info border border-2 border-info fs-5  p-2 rounded fw-semibold">
                                    Cancel
                                </a>
                            </button>
                        </div>
                    </div>
                </form>

                <form class="allForm" id="PhysicianBusinessInfoForm" method="post" onsubmit="return checkSign() && checkProfileImage() && PhysicianBusinessInfoEdit(@Model.callId)">
                    <input asp-for="@Model.AspId" class="editfield4" id="aspId4" hidden />
                    <input asp-for="@Model.PhysicianId" class="editfield4" id="physicianId4" hidden />
                    <div class="row">
                        <div class="col-12 mt-4">
                            <h5>Provider Profile</h5>
                        </div>
                        <div class="col-xl-6 mt-2">
                            <div class="form-floating">
                                <input type="text" asp-for="@Model.BusinessName" class="form-control editfield4" id="floatingBusinessName" placeholder="Business Name">
                                <label for="floatingBusinessName" class="text-secondary">Business Name</label>
                                <span class="text-danger" asp-validation-for="BusinessName"></span>
                            </div>
                        </div>
                        <div class="col-xl-6 mt-4 mt-xl-2">
                            <div class="form-floating">
                                <input type="text" asp-for="@Model.BusinessWebsite" class="form-control editfield4" id="floatingBusinessWebsite" placeholder="Business Website">
                                <label for="floatingBusinessWebsite" class="text-secondary">Business Website</label>
                                <span class="text-danger" asp-validation-for="BusinessWebsite"></span>
                            </div>
                        </div>
                        <div class="col-xl-6 mt-4 ">
                             <div class="col input-group ">                 
                                <input type="file" asp-for="@Model.Photo" class="form-control filestyle editfield4" id="inputGroupFile02"  onchange="return  checkProfileImage() && updateFileName(this)  ">
                                @* <input id="upload-name"  class="form-control inputFile filestyle" type="text" value="Select Photo" aria-label="readonly input example" readonly> *@
                                <label class="input-group-text btn btn-info pt-3 upload text-white" for="inputGroupFile02"> <i class="bi bi-cloud-arrow-up"></i> Upload </label>
                            </div>
                            <span class="text-danger" id="errorSpanProfile"></span>
                            @if (Model.PhotoValue != null)
                            {
                                <div class="d-flex justify-content-center w-100 mt-2 mb-2">
                                    <img src="~/content/@Model.PhysicianId/Profile.png" width="200px" alt="Profile">
                                </div>
                            }
                        </div>
                        <div class="col-xl-6 mt-4">
                            <div class="col input-group">
                                <input type="file" asp-for="@Model.Signature" class="form-control filestyle editfield4" id="inputGroupFile03"  onchange="checkSign()">
                                @* <input id="upload-name2" class="form-control inputFile filestyle" type="text" value="Select Signature" aria-label="readonly input example" readonly> *@
                                <label class="input-group-text btn btn-info pt-3 upload text-white" for="inputGroupFile03"> <i class="bi bi-cloud-arrow-up"></i> Upload </label>
                            </div>
                            <span class="text-danger" id="errorSpanProfile2"></span>
                            @if (Model.SignatureValue != null)
                            {
                                <div class="d-flex justify-content-center w-100 mt-2 mb-2">
                                    <img src="~/content/@Model.PhysicianId/Signature.png" width="200px" alt="Signature">
                                </div>
                            }
                        </div>
                        @if (Model.callId != 3)
                        {
                            <div class="col-xl-12 mt-4">
                                <div class="form-floating">
                                    <textarea asp-for="@Model.AdminNotes" class="form-control  h-100 editfield4" rows="6" placeholder="Admin Notes" id="floatingTextarea2"></textarea>
                                    <label for="floatingTextarea2" class="text-secondary">Admin Notes</label>
                                     <span class="text-danger" asp-validation-for="AdminNotes"></span>
                                </div>
                            </div>
                        }
                        <div class="col-12 d-flex justify-content-end  mt-4">
                            @if (Model.callId != 3)
                            {
                                <button type="button" id="editbtn4" class="border-0 bg-transparent">
                                    <a class="btn  btn-info text-white fs-5  py-2 px-4 rounded fw-semibold me-2">
                                        Edit
                                    </a>
                                </button>
                            }
                            <button type="submit" id="savebtn4" class="border-0 bg-transparent" style="display:none">
                                <a class="btn  btn-info text-white fs-5  py-2 px-4 rounded fw-semibold me-2">
                                    Save
                                </a>
                            </button>
                            <button type="reset" id="cancelbtn4" class="border-0 bg-transparent" style="display:none">
                                <a class="btn btn-outline-info border border-2 border-info fs-5  p-2 rounded fw-semibold">
                                    Cancel
                                </a>
                            </button>
                        </div>
                    </div>
                </form>


                <hr class="w-100" />
                <form method="post" id="OnboardingEditForm" onsubmit="return checkOnboardingFiles(1) && checkOnboardingFiles(2) && checkOnboardingFiles(3) && checkOnboardingFiles(4) && UpdateOnBoarding(@Model.callId)">
                    <input asp-for="@Model.AspId" id="aspId5" hidden/>
                    <div class="pt-4">
                        <h4 class="mb-4">Onboarding</h4>
                        <div class="mb-3 d-md-flex align-items-center justify-content-between onBoardContainer">
                            <div class="form-check d-flex">
                                <input class="form-check-input ms-1 p-2 Onboarding" type="checkbox" value="1" id="Onboarding1" @(Model.IsContractorAgreement ? "checked" : "")>
                                <label class="form-check-label ms-3 fw-bolder" for="Onboarding1">
                                    Independent Contractor Agreement
                                </label>
                            </div>
                            <span class="text-danger" id="errorSpan1"></span>
                            <div class="d-flex justify-content-center">
                                <a type="button" href="~/content/@Model.PhysicianId/Independent_Contractor.pdf" target="_blank" id="OnboardingView1" class="btn btn-info text-white me-2" style="display:none">
                                    View
                                </a>
                                @if (Model.callId != 3)
                                {
                                <input type="file" hidden class="form-control onBoardInpFile" asp-for="@Model.ContractorAgreement" id="inputOnboardFile1" onchange="checkOnboardingFiles(1)">
                                <label class="input-group-text me-3 input-group-text bg-info text-white" for="inputOnboardFile1">Upload</label> 
                                }
                            </div>
                        </div>
                        <div class="mb-3 d-md-flex align-items-center justify-content-between onBoardContainer">
                            <div class="form-check d-flex">
                                <input class="form-check-input ms-1 p-2 Onboarding" type="checkbox" value="2" id="Onboarding2" @(Model.IsBackgroundCheck ? "checked" : "")>
                                <label class="form-check-label ms-3 fw-bolder" for="Onboarding2">
                                    Background Check
                                </label>
                            </div>
                            <span class="text-danger" id="errorSpan2"></span>
                            <div class="d-flex justify-content-center">
                                <a type="button" href="~/content/@Model.PhysicianId/Background.pdf" target="_blank" id="OnboardingView2" class="btn btn-info text-white me-2" style="display:none">
                                    View
                                </a>
                                @if (Model.callId != 3)
                                {
                                <input type="file" hidden class="form-control onBoardInpFile" asp-for="@Model.BackgroundCheck" id="inputOnboardFile2" onchange="checkOnboardingFiles(2)">
                                <label class="input-group-text me-3 input-group-text bg-info text-white" for="inputOnboardFile2">Upload</label>
                                }
                            </div>
                        </div>
                        <div class="mb-3 d-flex align-items-center justify-content-between onBoardContainer">
                            <div class="form-check d-flex">
                                <input class="form-check-input ms-1 p-2 Onboarding" type="checkbox" value="3" id="Onboarding3" @(Model.IsHIPAA ? "checked" : "")>
                                <label class="form-check-label ms-3 fw-bolder" for="Onboarding3">
                                    HIPAA Compliance
                                </label>
                            </div>
                            <span class="text-danger" id="errorSpan3"></span>
                            <div class="d-flex justify-content-center">
                                <a type="button" href="~/content/@Model.PhysicianId/HIPAA.pdf" target="_blank" id="OnboardingView3" class="btn btn-info text-white me-2" style="display:none">
                                    View
                                </a>
                                @if (Model.callId != 3)
                                {
                                <input type="file" hidden class="form-control onBoardInpFile" asp-for="@Model.HIPAA" id="inputOnboardFile3" onchange="checkOnboardingFiles(3)">
                                <label class="input-group-text me-3 input-group-text bg-info text-white" for="inputOnboardFile3">Upload</label>
                                }
                            </div>
                        </div>
                        <div class="mb-3 d-flex align-items-center justify-content-between onBoardContainer">
                            <div class="form-check d-flex">
                                <input class="form-check-input ms-1 p-2 Onboarding" type="checkbox" value="4" id="Onboarding4" @(Model.IsNonDisclosure ? "checked" : "")>
                                <label class="form-check-label ms-3 fw-bolder" for="Onboarding4">
                                    Non-disclouser Agreement
                                </label>
                            </div>
                            <span class="text-danger" id="errorSpan4"></span>
                            <div class="d-flex justify-content-center">
                                <a type="button" href="~/content/@Model.PhysicianId/Non_Disclosure.pdf" target="_blank" id="OnboardingView4" class="btn btn-info text-white me-2" style="display:none">
                                    View
                                </a>
                                @if (Model.callId != 3)
                                {
                                <input type="file" hidden class="form-control onBoardInpFile" asp-for="@Model.NonDisclosure" id="inputOnboardFile4" onchange="checkOnboardingFiles(4)">
                                <label class="input-group-text me-3 input-group-text bg-info text-white" for="inputOnboardFile4">Upload</label>  
                                }
                            </div>
                        </div>
                        <div class="mb-3 d-flex align-items-center justify-content-between onBoardContainer">
                            <div class="form-check d-flex">
                                <input class="form-check-input ms-1 p-2 Onboarding" type="checkbox" value="5" id="Onboarding5" @(Model.IsLicenseDocument ? "checked" : "")>
                                <label class="form-check-label ms-3 fw-bolder" for="Onboarding5">
                                    License Document
                                </label>
                            </div>
                            <span class="text-danger" id="errorSpan5"></span>
                            <div class="d-flex justify-content-center">
                                <a type="button" href="~/content/@Model.PhysicianId/Licence.pdf" target="_blank" id="OnboardingView5" class="btn btn-info text-white me-2" style="display:none">
                                    View
                                </a>
                                @if (Model.callId != 3)
                                {
                                <input type="file" hidden class="form-control onBoardInpFile" asp-for="@Model.LicenseDocument" id="inputOnboardFile5" onchange="checkOnboardingFiles(5)">
                                <label class="input-group-text me-3 input-group-text bg-info text-white" for="inputOnboardFile5">Upload</label>
                                }
                            </div>
                        </div>
                    </div>

                    <hr class="w-100" />
                    @if (Model.callId != 3)
                    {
