@model HelloDoc.DAL.ViewModels.Encountervm;



<style>


    h1 {
        text-align: center;
        color: #0dcaf0;
    }

    span {
        font-weight: bolder;
    }

    .data {
        margin-bottom: 10px;
        font-size: large;
    }

</style>



<div class="encounterForm h-100 p-3">
    <h1>Encounter Form Details Data</h1>
    <div class="details p-3">
        <div class="data"><span>First Name: </span> @Model.FirstName </div>
        <div class="data"><span>Last Name: </span> @Model.LastName </div>
        <div class="data"><span>Location: </span> @Model.Location </div>
        <div class="data"><span>Date of Birth: </span> @Model.BirthDate </div>
        @* <div class="data"><span>Date of Request: </span> @Model.Date </div> *@
        <div class="data"><span>Phone: </span> @Model.PhoneNumber </div>
        <div class="data"><span>Email: </span> @Model.Email </div>
        <div class="data"><span>History of Present Illness or Injury: </span> @Model.HistoryOfPresentIllness </div>
        <div class="data"><span>Medical History: </span> @Model.MedicalHistory </div>
        <div class="data"><span>Medications: </span> @Model.Medications </div>
        <div class="data"><span>Allergies: </span> @Model.Allergies </div>
        <div class="data"><span>Temp: </span> @Model.Temperature </div>
        <div class="data"><span>HR: </span> @Model.HR </div>
        <div class="data"><span>RR: </span> @Model.RR </div>
        <div class="data"><span>Blood Pressure Diastolic: </span> @Model.BloodPressureDiastolic </div>
        <div class="data"><span>Blood Pressure Systolic: </span> @Model.BloodPressureSystolic </div>
        <div class="data"><span>O2: </span> @Model.O2 </div>
        <div class="data"><span>Heent: </span> @Model.HEENT </div>
        <div class="data"><span>Pain: </span> @Model.Pain </div>
        <div class="data"><span>CV: </span> @Model.CV </div>
        <div class="data"><span>Chest: </span> @Model.Chest </div>
        <div class="data"><span>ABD: </span> @Model.ABD </div>
        <div class="data"><span>Extremities: </span> @Model.Extr </div>
        <div class="data"><span>Skin: </span> @Model.Skin </div>
        <div class="data"><span>Neuro: </span> @Model.Neuro </div>
        <div class="data"><span>Other: </span> @Model.Other </div>
        <div class="data"><span>Diagnosis: </span> @Model.Diagnosis </div>
        <div class="data"><span>Treatment Plan: </span> @Model.TreatmentPlan </div>
        <div class="data"><span>Medical Dispensed: </span> @Model.MedicationDispensed </div>
        <div class="data"><span>Procedures: </span> @Model.Procedures </div>
        <div class="data followup"><span>FollowUp: </span> @Model.FollowUp </div>
    </div>
</div>



